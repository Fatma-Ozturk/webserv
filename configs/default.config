http {
    server {
    	server_name default default1;
    	listen 127.0.0.1:8180;

    	location /					
    	{
			return 301 http://google.com;
			error_page 404 ./tests/test1/error404.html;
    		allow_methods GET POST DELETE;
    		root ./tests/test1/;
			index index.html index2.html;
		}
    
    	location /virtual/
    	{
    		allow_methods GET POST DELETE;
    		request_max_body_size	42;
    		root ./tests/test2/;
    		index index.html index2.html;
    		auth_key selim:1234;
			cgi_pass /usr/bin/php;
    	}
    }
    
    server
    {
    	server_name second;
    	listen 127.0.0.1:8181;
    
    	location /
    	{
    		allow_methods GET;
    		root ./tests/test1/;
    		index second.html;
			cgi_pass /usr/bin/php;
		}
		
    	location /abc/
    	{
    		allow_methods GET;
    		index second.html;
    	}
    }
    
    server
    {
    	server_name google.com;
    	listen 127.0.0.1:8282;
    
    	location /
    	{
    		allow_methods GET;
    		return 301 http://google.com;
			cgi_pass /usr/bin/php;
		}
    }
}
